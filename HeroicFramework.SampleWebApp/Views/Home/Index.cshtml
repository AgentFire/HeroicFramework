@model HeroicFramework.SampleWebApp.Models.FooViewModel[]

<!DOCTYPE html>
<html>
<head>
    <!-- Latest compiled and minified CSS -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" integrity="sha384-1q8mTJOASx8j1Au+a5WDVnPi2lkFfwwEAa8hDDdjZlpLegxhjVME1fgjWPGmkzs7" crossorigin="anonymous">
    <title>Foo Sample</title>
</head>
<body>
    <div class="container">
        <h1>Foo Sample</h1>
        <p class="lead">This sample shows off both Heroic.AutoMapper and Heroic.Web.IoC. The data below was pulled from an injected provider and projected using AutoMapper.</p>
        <div>
            @foreach (var foo in Model)
            {
                <div class="panel panel-default">
                    <div class="panel-heading">
                        @foo.Id
                    </div>
                    <div class="panel-body">
                        @foo.Name
                    </div>
                </div>
            }
        </div>

    </div>
</body>
</html>